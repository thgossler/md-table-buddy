        "label": "Table Buddy"
      }
    ],
    "keybindings": [
      {
        "command": "md-table-buddy.navigateNextCell",
        "key": "tab",
        "when": "editorLangId == markdown && editorTextFocus && !editorTabMovesFocus"
      },
      {
        "command": "md-table-buddy.navigatePrevCell",
        "key": "shift+tab",
        "when": "editorLangId == markdown && editorTextFocus && !editorTabMovesFocus"
      },
      {
        "command": "md-table-buddy.navigateCellBelow",
        "key": "enter",
        "when": "editorLangId == markdown && editorTextFocus && !suggestWidgetVisible"
      }
    ],
    "configuration": {
      "title": "Markdown Table Buddy",
      "properties": {
        "md-table-buddy.fileExtensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            ".md"
          ],
          "description": "File extensions to treat as Markdown files (e.g., ['.md', '.mdx', '.markdown'])"
        },
        "md-table-buddy.compactTable.cellPadding": {
          "type": "boolean",
          "default": true,
          "description": "Add a space at the start and end of each cell content"
        },
        "md-table-buddy.compactTable.separatorPadding": {
          "type": "boolean",
          "default": true,
          "description": "Add a space at the start and end of separator row cells"
        },
        "md-table-buddy.compactTable.alignSeparatorWithHeader": {
          "type": "boolean",
          "default": true,
          "description": "Align separator row column widths with the header text widths"
        },
        "md-table-buddy.compactTable.keepSeparatorRatios": {
          "type": "boolean",
          "default": false,
          "description": "Preserve original separator length ratios (for Pandoc column width hints)"
        },
        "md-table-buddy.formatTable.maxWidth": {
          "type": "number",
          "default": 0,
          "description": "Maximum table width (0 = no limit)"
        },
        "md-table-buddy.formatTable.cellPadding": {
          "type": "boolean",
          "default": true,
          "description": "Add a space at the start and end of each cell content"
        },
        "md-table-buddy.formatTable.separatorPadding": {
          "type": "boolean",
          "default": true,
          "description": "Add a space at the start and end of separator row cells"
        },
        "md-table-buddy.formatTable.preserveAlignment": {
          "type": "boolean",
          "default": true,
          "description": "Preserve existing column alignment markers"
        },
        "md-table-buddy.formatTable.keepSeparatorRatios": {
          "type": "boolean",
          "default": false,
          "description": "Preserve original separator length ratios (for Pandoc column width hints)"
        },
        "md-table-buddy.rowNumbers.startNumber": {
          "type": "number",
          "default": 1,
          "description": "Starting number for row numbering"
        },
        "md-table-buddy.rowNumbers.headerText": {
          "type": "string",
          "default": "#",
          "description": "Header text for the row number column"
        },
        "md-table-buddy.rowNumbers.alignment": {
          "type": "string",
          "enum": [
            "left",
            "center",
            "right"
          ],
          "default": "right",
          "description": "Alignment for the row number column"
        },
        "md-table-buddy.sort.keepHeaderRow": {
          "type": "boolean",
          "default": true,
          "description": "Keep header row in place when sorting"
        },
        "md-table-buddy.csv.delimiter": {
          "type": "string",
          "default": ",",
          "description": "CSV delimiter character"
        },
        "md-table-buddy.csv.hasHeader": {
          "type": "boolean",
          "default": true,
          "description": "Treat first row of CSV as header"
        },
        "md-table-buddy.csv.trimCells": {
          "type": "boolean",
          "default": true,
          "description": "Trim whitespace from CSV cells"
        },
        "md-table-buddy.csv.quoteStrings": {
          "type": "string",
          "enum": [
            "always",
            "auto",
            "never"
          ],
          "default": "auto",
          "description": "When to quote strings in CSV output"
        },
        "md-table-buddy.csv.includeHeader": {
          "type": "boolean",
          "default": true,
          "description": "Include header row when converting to CSV"
        },
        "md-table-buddy.formatOnSave": {
          "type": "boolean",
          "default": false,
          "description": "Automatically format tables on save (respects each table's current mode: compact or formatted)"
        },
        "md-table-buddy.autoInsertColumnOnTab": {
          "type": "boolean",
          "default": true,
          "description": "Automatically insert a new column when pressing Tab at the last cell of any row"
        },
        "md-table-buddy.autoInsertRowOnEnter": {
          "type": "boolean",
          "default": true,
          "description": "Automatically insert a new row when pressing Enter in the last row"
        },
        "md-table-buddy.ignoreCodeBlocks": {
          "type": "boolean",
          "default": true,
          "description": "Ignore tables inside code blocks (``` or ~~~)"
        }
      }
    }
