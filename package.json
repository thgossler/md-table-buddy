        "label": "Table Buddy"
      }
    ],
    "keybindings": [
      {
        "command": "md-table-buddy.navigateNextCell",
        "key": "tab",
        "when": "editorLangId == markdown && editorTextFocus && !editorTabMovesFocus"
      },
      {
        "command": "md-table-buddy.navigatePrevCell",
        "key": "shift+tab",
        "when": "editorLangId == markdown && editorTextFocus && !editorTabMovesFocus"
      },
      {
        "command": "md-table-buddy.navigateCellBelow",
        "key": "enter",
        "when": "editorLangId == markdown && editorTextFocus && !suggestWidgetVisible"
      }
    ],
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "md-table-buddy.fileExtensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            ".md"
          ],
          "description": "%configuration.fileExtensions%"
        },
        "md-table-buddy.compactTable.cellPadding": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.compactTable.cellPadding%"
        },
        "md-table-buddy.compactTable.separatorPadding": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.compactTable.separatorPadding%"
        },
        "md-table-buddy.compactTable.alignSeparatorWithHeader": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.compactTable.alignSeparatorWithHeader%"
        },
        "md-table-buddy.compactTable.keepSeparatorRatios": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.compactTable.keepSeparatorRatios%"
        },
        "md-table-buddy.formatTable.maxWidth": {
          "type": "number",
          "default": 0,
          "description": "%configuration.formatTable.maxWidth%"
        },
        "md-table-buddy.formatTable.cellPadding": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.formatTable.cellPadding%"
        },
        "md-table-buddy.formatTable.separatorPadding": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.formatTable.separatorPadding%"
        },
        "md-table-buddy.formatTable.preserveAlignment": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.formatTable.preserveAlignment%"
        },
        "md-table-buddy.formatTable.keepSeparatorRatios": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.formatTable.keepSeparatorRatios%"
        },
        "md-table-buddy.rowNumbers.startNumber": {
          "type": "number",
          "default": 1,
          "description": "%configuration.rowNumbers.startNumber%"
        },
        "md-table-buddy.rowNumbers.headerText": {
          "type": "string",
          "default": "#",
          "description": "%configuration.rowNumbers.headerText%"
        },
        "md-table-buddy.rowNumbers.alignment": {
          "type": "string",
          "enum": [
            "left",
            "center",
            "right"
          ],
          "default": "right",
          "description": "%configuration.rowNumbers.alignment%"
        },
        "md-table-buddy.sort.keepHeaderRow": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.sort.keepHeaderRow%"
        },
        "md-table-buddy.csv.delimiter": {
          "type": "string",
          "default": ",",
          "description": "%configuration.csv.delimiter%"
        },
        "md-table-buddy.csv.hasHeader": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.csv.hasHeader%"
        },
        "md-table-buddy.csv.trimCells": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.csv.trimCells%"
        },
        "md-table-buddy.csv.quoteStrings": {
          "type": "string",
          "enum": [
            "always",
            "auto",
            "never"
          ],
          "default": "auto",
          "description": "%configuration.csv.quoteStrings%"
        },
        "md-table-buddy.csv.includeHeader": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.csv.includeHeader%"
        },
        "md-table-buddy.formatOnSave": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.formatOnSave%"
        },
        "md-table-buddy.autoInsertColumnOnTab": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.autoInsertColumnOnTab%"
        },
        "md-table-buddy.autoInsertRowOnEnter": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.autoInsertRowOnEnter%"
        },
        "md-table-buddy.ignoreCodeBlocks": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.ignoreCodeBlocks%"
        }
